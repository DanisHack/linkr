{
  "name": "linkr",
  "version": "1.0.0",
  "description": "Beautiful link shortening",
  "scripts": {
    "build": "npm run build-css && npm run build-js",
    "build-js": "mkdir -p frontend/static/dist && webpack --config frontend/config/webpack.js",
    "build-css": "mkdir -p frontend/static/dist && sass --style compressed frontend/styles/main.scss:frontend/static/dist/main.css",
    "build-js-watch": "mkdir -p frontend/static/dist && webpack --config frontend/config/webpack.js --watch --colors",
    "build-css-watch": "mkdir -p frontend/static/dist && sass --watch --style compressed frontend/styles/main.scss:frontend/static/dist/main.css",
    "start": "python linkr.py",
    "lint": "eslint frontend/app test/frontend",
    "test": "npm run lint && node test/frontend | tap-spec",
    "cover": "istanbul cover test/frontend --print detail",
    "coveralls": "istanbul cover test/frontend --print detail && cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "TODO"
  },
  "author": "Kevin Lin <developer@kevinlin.info>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.11.4",
    "body-parser": "^1.15.2",
    "browser-request": "^0.3.3",
    "clone": "^2.1.0",
    "cookie-parser": "^1.4.3",
    "copy-to-clipboard": "^3.0.5",
    "deep-extend": "^0.4.1",
    "dottie": "^1.1.1",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "pug": "^2.0.0-beta4",
    "randomstring": "^1.1.5",
    "range": "0.0.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-helmet": "^3.1.0",
    "react-router": "^2.6.1",
    "safe-json-parse": "^4.0.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.11.15",
    "enzyme": "^2.6.0",
    "eslint": "^3.3.1",
    "eslint-config-uber-jsx": "^2.0.0",
    "eslint-plugin-react": "^6.1.2",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.9.1",
    "json-loader": "^0.5.4",
    "react-addons-test-utils": "^15.4.1",
    "sinon": "^1.17.6",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "webpack": "^1.13.1"
  }
}
